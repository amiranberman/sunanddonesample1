{
  "name": "sun-and-done",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "yarn db:generate && next build",
    "db:connect": "pscale connect sun-and-done dev --port 3309 --org sun-and-done",
    "db:generate": "yarn pnpify prisma generate",
    "db:push": "yarn pnpify prisma db push",
    "db:update": "yarn db:generate && yarn db:push",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed"
  },
